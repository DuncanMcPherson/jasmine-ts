{
  "name": "jasmine-ts",
  "version": "1.0.0",
  "description": "Execute jasmine with ts-node",
  "main": "lib/index.js",
  "bin": "lib/index.js",
  "scripts": {
    "precompile": "rimraf lib",
    "compile": "tsc --project tsconfig.json",
    "lint": "tslint -p tsconfig.json",
    "jasmineTs": "node lib/index.js --config=spec/jasmine.json",
    "test": "npm run lint && npm run compile && npm run jasmineTs",
    "prepublishOnly": "npm run compile",
    "build": "npm run compile",
    "prepack": "npm run build",
    "prerealease": "npm pack",
    "release": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duncanmcpherson/jasmine-ts.git"
  },
  "keywords": [
    "node",
    "testing",
    "tests",
    "jasmine",
    "typescript"
  ],
  "author": "ez-budgets",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/duncanmcpherson/jasmine-ts/issues"
  },
  "homepage": "https://github.com/duncanmcpherson/jasmine-ts#readme",
  "files": [
    "lib"
  ],
  "dependencies": {
    "yargs": "^17.0.1"
  },
  "peerDependencies": {
    "jasmine": ">=5.1.0",
    "ts-node": ">=10.9.2",
    "typescript": ">=5.3.3"
  },
  "devDependencies": {
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.10.5",
    "@types/yargs": "^17.0.0",
    "dotenv": "^16.3.1",
    "jasmine": "^5.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "rimraf": "^5.0.5",
    "standard-version": "^9.1.0",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "typescript": "^5.3.3"
  }
}
